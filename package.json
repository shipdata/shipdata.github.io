{
  "name": "kancolle-recipe-generator",
  "version": "1.0.0",
  "private": true,
  "description": "kancolle-recipe-generator",
  "homepage": "https://github.com/wantora/kancolle-recipe-generator#readme",
  "bugs": {
    "url": "https://github.com/wantora/kancolle-recipe-generator/issues"
  },
  "license": "MIT",
  "author": "wantora <wantora@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/wantora/kancolle-recipe-generator.git"
  },
  "scripts": {
    "build": "npm-run-all -s clean build:js",
    "build:js": "webpack --env.production true",
    "clean": "rimraf dist",
    "db:download": "mkdirp ./db && wget https://poi.0u0.moe/dump/createitemrecords.gz -O ./db/createitemrecords.gz --no-clobber",
    "db:restore": "mongorestore --port 27018 --drop --archive --gzip < ./db/createitemrecords.gz",
    "db:start": "mkdirp ./db/mongo && mongod --port 27018 --dbpath ./db/mongo",
    "db:updateItems": "node ./scripts/updateItems.js",
    "deploy": "npm-run-all -s build deploy:gh",
    "deploy:gh": "gh-pages -d ./dist",
    "lint": "eslint .",
    "start": "webpack-dev-server --content-base ./src/www --inline --open"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "classnames": "^2.2.5",
    "core-js": "^2.5.3",
    "events": "^2.0.0",
    "lodash": "^4.17.5",
    "numeral": "^2.0.6",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-bootstrap": "^0.32.1",
    "react-dom": "^16.2.0",
    "regenerator-runtime": "^0.11.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "copy-webpack-plugin": "^4.5.0",
    "css-loader": "^0.28.10",
    "eslint": "^4.18.2",
    "eslint-config-wantora": "github:wantora/eslint-config-wantora#v1.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "gh-pages": "^1.1.0",
    "mkdirp": "^0.5.1",
    "mongodb": "^3.0.4",
    "npm-run-all": "^4.1.2",
    "rimraf": "^2.6.2",
    "style-loader": "^0.20.2",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.11.2"
  }
}
